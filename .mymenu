
[system_tools]
 tree          : directory list
 tty-clock     : terminal digital clock
 tmux          : terminal multiplexer
 screenfetch   : print system information
 htop          : system process monitor
 apvlv         : pdf viewer
 scrot         : screen capture tool

[packages]
 pacup         : update packages
 yup           : update aur packages

[internet]
 mutt          : email client
 mcabber       : instant message client
 weechat       : irc client    
 turses        : twitter client
 rtv           : reddit client
 pianobar      : pandora radio client

[games]
 ninvader      : space invaders clone
 brogue        : roguelike adventure
 dwarffortress : dwarf simulator
 openarena     : quake III clone
